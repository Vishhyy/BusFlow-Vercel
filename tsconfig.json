{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "module": "esnext"
  },
  "include": ["src"],
  "exclude": ["node_modules", ".svelte-kit", "build"]
}
